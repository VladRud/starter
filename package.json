    "angular-chart.js": "^1.1.1",
    "angular-cookies": "^1.7.2",
    "angular-moment": "^1.2.0",
    "angular-resource": "^1.7.2",
    "angular-sanitize": "^1.7.2",
    "angular-spinner": "^1.0.1",
    "angular-ui-bootstrap": "^2.5.6",
    "angular-validation-match": "^1.9.0",
    "body-parser": "^1.18.3",
    "bootstrap": "~3.3",
    "bootstrap-daterangepicker": "^3.0.1",
    "botbuilder": "^3.15.0",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.2",
    "connect-livereload": "^0.6.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "fs-extra": "^6.0.1",
    "googleapis": "~2.1.7",
    "jquery": "^3.3.1",
