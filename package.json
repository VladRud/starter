{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build:serve": "npm run build:dev && npm run serve",
    "build:dev": "npm run client:build:dev",
    "serve": "NODE_ENV=development nodemon dist/server & npm run watch",
    "client:build:dev": "npm run clean && NODE_ENV=development webpack && npm run copy:client",
    "build:start": "npm run build && npm run start",
    "build": "npm run client:build:prod && npm run server:build",
    "start": "NODE_ENV=production node dist/server",
    "client:build:prod": "npm run clean && NODE_ENV=production webpack && npm run copy:client",
    "server:build": "NODE_ENV=production webpack --config webpack-server.config.js && npm run copy:server",
    "watch": "npm run client:watch & npm run server:watch",
    "client:watch": "NODE_ENV=development webpack --watch",
    "server:watch": "NODE_ENV=development webpack --watch --config webpack-server.config.js",
    "clean": "rimraf dist/*",
    "copy:server": "cp .credentials dist -r",
    "copy:client": "cp -t dist/client client/favicon.ico client/robots.txt"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMap": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
